{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "assets/*": ["src/assets/*"],
      "components/*": ["src/components/*"],
      "content/*": ["src/content/*"],
      "layouts/*": ["src/layouts/*"],
      "lib/*": ["src/lib/*"],
      "pages/*": ["src/pages/*"],
      "public/*": ["src/public/*"],
      "styles/*": ["src/styles/*"]
    }
  }
}
