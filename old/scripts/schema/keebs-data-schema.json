{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "owner",
    "name",
    "pcb",
    "switches",
    "keyset",
    "description",
    "photos"
  ],
  "properties": {
    "owner": {
      "$id": "#/properties/owner",
      "type": "string",
      "title": "The Owner Schema",
      "minLength": 1,
      "examples": [
        "Zekth"
      ],
      "pattern": "^(.*)$"
    },
    "name": {
      "$id": "#/properties/name",
      "type": "string",
      "title": "The Name Schema",
      "minLength": 1,
      "examples": [
        "der 60%"
      ],
      "pattern": "^(.*)$"
    },
    "pcb": {
      "$id": "#/properties/pcb",
      "type": "string",
      "title": "The Pcb Schema",
      "minLength": 1,
      "examples": [
        "KP XD64"
      ],
      "pattern": "^(.*)$"
    },
    "switches": {
      "$id": "#/properties/switches",
      "type": "string",
      "title": "The Switches Schema",
      "minLength": 1,
      "examples": [
        "Kailh Box Brown"
      ],
      "pattern": "^(.*)$"
    },
    "keyset": {
      "$id": "#/properties/keyset",
      "type": "string",
      "title": "The Keyset Schema",
      "minLength": 1,
      "examples": [
        "GMK Plum"
      ],
      "pattern": "^(.*)$"
    },
    "description": {
      "$id": "#/properties/description",
      "type": "string",
      "title": "The Description Schema",
      "minLength": 1,
      "examples": [
        "c'est un clavier avec des touches"
      ],
      "pattern": "^(.*)$"
    },
    "photos": {
      "$id": "#/properties/photos",
      "type": "array",
      "title": "The Photos Schema",
      "items": {
        "$id": "#/properties/photos/items",
        "type": "string",
      "minLength": 1,
      "title": "The photo filename Schema",
        "examples": [
          "zekth-60-1.jpg"
        ],
        "pattern": "^(.*)$"
      }
    }
  }
}
