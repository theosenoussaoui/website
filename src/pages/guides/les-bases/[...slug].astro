---
import PostsLayout from "layouts/PostsLayout.astro";
import { getAllFilesFromSubDir } from "lib/getContentFromSubdirectory";

export async function getStaticPaths() {
  return await getAllFilesFromSubDir("guides", "les-bases");
}

const { entry } = Astro.props;
const { Content } = await entry.render();
---

<PostsLayout>
  <Content />
</PostsLayout>
