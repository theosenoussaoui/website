---
import { getCollection, getEntryBySlug } from "astro:content";
import MeetupLayout from "layouts/MeetupLayout.astro";

export async function getStaticPaths() {
    const meetupEntries = await getCollection("meetup");

    console.log(meetupEntries);
    return meetupEntries.map((entry) => ({
        params: { slug: entry.slug },
        props: { entry },
    }));
}

const { entry } = Astro.props;
const { Content } = await entry.render();
---

<MeetupLayout title="Meetup index page">
    <Content slot="article" />
</MeetupLayout>
