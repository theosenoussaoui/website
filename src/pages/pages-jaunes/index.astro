---
import { getEntryBySlug } from "astro:content";

import PostsLayout from "layouts/PostsLayout.astro";

const entry = await getEntryBySlug("pages-jaunes", "index");
const { Content } = await entry.render();

const { title, description, tags, image, navigationTitle } = entry.data;
---

<PostsLayout title={title}>
    <Content slot="article" />
</PostsLayout>
